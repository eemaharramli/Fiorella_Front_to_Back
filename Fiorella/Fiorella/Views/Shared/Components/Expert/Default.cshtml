@model Task<List<Fiorella.Models.Expert>>

@foreach (var item in Model.Result)
{
    <div class="col-md-6 col-lg-3">
        <div class="item text-center">
            <div class="img">
                <img src="~/img/@item.Image" class="img-fluid rounded-circle" alt="">
            </div>
            <div class="text mt-3">
                <h6>@item.Fullname</h6>
                <p class="text-black-50">@item.ExpertJob.Name</p>
            </div>
        </div>
    </div>
}